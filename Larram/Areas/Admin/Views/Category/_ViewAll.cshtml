@model PaginatedList<Larram.Models.Category>


        <table class="table table-striped table-bordered text-center Roboto" style="font-size:100%;">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">
                    <a class="sortLink" asp-action="Index" asp-route-orderBy="@ViewData["NameSortParm"]" asp-route-currentFilter="@ViewData["CurrentFilter"]">Nazwa</a>
                    </th>
                    <th scope="col">Akcje</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var category in Model)
                {
                    <tr>

                        <td style="width:70%;">@category.Name</td>
                        <td style="width:30%;">
                            <a onclick="showInPopup('@Url.Action("Upsert","Category",new {id=category.Id},Context.Request.Scheme)')" class="text-dark"><i style="font-size:22px" data-feather="edit"></i></a>
                            <a onclick="showInPopup('@Url.Action("Delete","Category",new {id=category.Id},Context.Request.Scheme)')" class="text-dark"><i style="font-size:22px" data-feather="trash-2"></i></a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
       