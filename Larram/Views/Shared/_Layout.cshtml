<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Larram</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/StyleSheet.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-dark bg-dark box-shadow p-0">
            <div class="container-fluid">
                <a class="navbar-brand quickSand" asp-area="" asp-controller="Home" asp-action="Index">Larram</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1 nav justify-content-center">
                        <li class="nav-item">
                            <a class="nav-link text-light Roboto" asp-area="" asp-controller="Home" asp-action="Index">WYPRZEDAŻ</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light Roboto" asp-area="" asp-controller="Home" asp-action="Privacy">KOBIETA</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light Roboto" asp-area="" asp-controller="Home" asp-action="Privacy">MĘŻCZYZNA</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-light Roboto" asp-area="" asp-controller="Home" asp-action="Privacy">NOWOŚCI</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid p-0 m-0">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted" style="line-height: 20px; background-color: lightgray;">
        <div class="container">
            <div class="d-flex justify-content-between">
                <ul class="footer-list">
                    <li><h5 class="Roboto text-dark"><b>PRZEWODNIK PO ZAKUPACH</b></h5></li>
                    <li class="footer-list-element Roboto"><a href="#">FAQ - NAJCZĘŚCIEJ ZADAWANE PYTANIA</a></li>
                    <li class="footer-list-element Roboto"><a href="#">JAK KUPOWAĆ</a></li>
                    <li class="footer-list-element Roboto"><a href="#">REKLAMACJE I ZWROTY</a></li>
                    <li class="footer-list-element Roboto"><a href="#">WYSYŁKA I PŁATNOŚĆ</a></li>
                </ul>
                <ul class="footer-list">
                    <li><h5 class="Roboto text-dark"><b>REGULAMINY</b></h5></li>
                    <li class="footer-list-element Roboto"><a href="#">REGULAMIN</a></li>
                    <li class="footer-list-element Roboto"><a href="#">POLITYKA PRYWATNOŚCI</a></li>
                </ul>
                <ul class="footer-list">
                    <li><h5 class="Roboto text-dark"><b>KONTAKT</b></h5></li>
                    <li class="footer-list-element Roboto"><a href="#">NAPISZ DO NAS</a></li>
                </ul>
                <ul class="footer-list">
                    <li><h5 class="Roboto text-dark" style="padding:0.1rem"><b>SOCIAL MEDIA</b></h5></li>
                    <li class="social-media-footer"><a href="#"><i style="font-size:26px" data-feather="facebook"></i></a></li>
                    <li class="social-media-footer"><a href="#"><i style="font-size:26px" data-feather="instagram"></i></a></li>
                    <li class="social-media-footer"><a href="#"><i style="font-size:26px" data-feather="twitter"></i></a></li>
                </ul>
            </div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://kit.fontawesome.com/4eeb01117f.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/feather-icons/dist/feather.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script>
        feather.replace({ width: '1em', height: '1em' })
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
